{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {

    },
    "overrides": [
        {
            "files":["*.js", "*.jsx"],
            "rules": {
                "react/no-unescaped-entities": "off",
                "react/react-in-jsx-scope": "off",
                "react/prop-types": "off"
            }
        }
    ]
}
